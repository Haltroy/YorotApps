<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="450"
		Width="500" Height="450"
        x:Class="YorotAppDesigner.EditorWindow"
        Title="Editor">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="*" />
			<ColumnDefinition Width="3*" />
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="40" />
			<RowDefinition Height="40" />
		</Grid.RowDefinitions>
		<TextBlock Grid.Row="0" Padding="5,15" Grid.Column="0">Name:</TextBlock>
		<TextBox Grid.Row="0" Grid.Column="1" Height="30" Watermark="Test" />
		<TextBlock Grid.Row="1" Grid.Column="0" Padding="5,15">Test 1:</TextBlock>
		<TextBox Grid.Row="1" Grid.Column="1" Height="30" Watermark="Test 1" />
	</Grid>
</Window>